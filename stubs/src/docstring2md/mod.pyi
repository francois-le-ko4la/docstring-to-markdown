from collections import deque
from docstring2md.ast_engine import ClassDef as ClassDef, FuncDef as FuncDef, ModuleDef as ModuleDef
from typing import Union

class PytMod:
    def __init__(self, module_name: str, priv: bool = ...) -> None: ...
    @property
    def module(self) -> str: ...
    @property
    def node_lst(self) -> deque[Union[ModuleDef, ClassDef, FuncDef, None]]: ...
    @property
    def pkg_main_docstring(self) -> deque[Union[ModuleDef, ClassDef, FuncDef, None]]: ...
    def ismodule(self) -> bool: ...
    def read(self) -> None: ...
